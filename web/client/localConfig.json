{
  "proxyUrl": "/mapstore/proxy/?url=",
  "geoStoreUrl": "/mapstore/rest/geostore",
  "printUrl": "https://demo.geo-solutions.it/geoserver/pdf/info.json",
  "initialState": {
    "defaultState": {
      "maptype": {
        "mapType": "{context.mode === 'desktop' ? 'openlayers' : 'leaflet'}"
      }
    }
  },
  "plugins": {
    "general-settings": [
      "GeneralSettings",
      {
        "name": "Stepper",
        "cfg": {
          "containerPosition": "footer",
          "steps": [
            {
              "id": "general-settings",
              "label": "General Settings",
              "selected": true
            },
            {
              "id": "configure-plugins",
              "label": "Configure Plugins"
            },
            {
              "id": "configure-map",
              "label": "Configure Map"
            }
          ],
          "next": {
            "label": "Next",
            "link": "/app-context/configure-plugins"
          },
          "back": {
            "label": "Back",
            "link": ""
          }
        }
      },
      {
        "name": "Navbar",
        "cfg": {
          "containerPosition": "header"
        }
      }
    ],
    "configure-plugins": [
      "ConfigurePlugins",
      {
        "name": "Stepper",
        "cfg": {
          "containerPosition": "footer",
          "steps": [
            {
              "id": "general-settings",
              "label": "General Settings",
              "passed": true
            },
            {
              "id": "configure-plugins",
              "label": "Configure Plugins",
              "selected": true
            },
            {
              "id": "configure-map",
              "label": "Configure Map"
            }
          ],
          "next": {
            "label": "Next",
            "link": "/app-context/configure-map"
          },
          "back": {
            "label": "Back",
            "link": "/app-context/general-settings"
          }
        }
      },
      {
        "name": "Navbar",
        "cfg": {
          "containerPosition": "header"
        }
      }
    ],
    "configure-map": [
      {
        "name": "Navbar",
        "cfg": {
          "containerPosition": "header"
        }
      },
      {
        "name": "Map",
        "cfg": {
          "mapOptions": {
            "openlayers": {
              "interactions": {
                "pinchRotate": false,
                "altShiftDragRotate": false
              }
            }
          }
        }
      },
      {
        "name": "Stepper",
        "cfg": {
          "containerPosition": "footer",
          "steps": [
            {
              "id": "general-settings",
              "label": "General Settings",
              "passed": true
            },
            {
              "id": "configure-plugins",
              "label": "Configure Plugins",
              "passed": true
            },
            {
              "id": "configure-map",
              "label": "Configure Map",
              "selected": true
            }
          ],
          "next": {
            "label": "Save",
            "link": "/app-context/configure-map"
          },
          "back": {
            "label": "Back",
            "link": "/app-context/configure-plugins"
          }
        }
      },
      "DrawerMenu",
      "BackgroundSelector",
      {
        "name": "CRSSelector",
        "cfg": {
          "additionalCRS": {},
          "filterAllowedCRS": [
            "EPSG:4326",
            "EPSG:3857"
          ]
        }
      },
      "Layout",
      "MapFooter",
      "AddGroup",
      {
        "name": "MetadataExplorer"
      },
      {
        "name": "TOC",
        "cfg": {
          "activateQueryTool": true,
          "activateAddLayerButton": true,
          "activateAddGroupButton": true,
          "activateMetedataTool": false
        }
      },
      "BurgerMenu",
      "OmniBar"
    ]
  }
}